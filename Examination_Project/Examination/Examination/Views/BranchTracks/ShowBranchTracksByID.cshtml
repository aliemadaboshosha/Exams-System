@model TrackBranch
@{
    ViewData["Title"] = "ShowBranchTracksByID";
}
<h1>ShowBranchTracksByID</h1>

<div style="text-align:center">
    <table class="table table-bordered">
        <thead class="thead-dark">
            <tr>
                <th>Branch Name</th>
                <th>Track ID</th>
                <th>Supervisor</th>
            </tr>
        </thead>
        <tbody>
             <tr>
                    <td>@Model.Branch.Name</td>
                    <td>@ViewBag.track.Name</td>
                    <td>@ViewBag.supervisor.Fname</td>
                </tr>
           
        </tbody>
    </table>
    <a class="btn btn-info mt-2 w-25" href="/Branch/Index">Back</a>

</div>
